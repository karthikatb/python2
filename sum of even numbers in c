// Write a C program to find sum of even numbers in a given range.(Exclusive of 
//limit)
//Test Data :
//Input range : 5  12
//Expected Output :
//Sum = 24
#include<stdio.h>
int main()
{
    int num,num1,i,sum=0;
    printf("enter the range:");
    scanf("%d%d",&num,&num1);
    for(i=num1;i<num;i++)
    {
        if (i%2==0)
        {
            sum=sum+i;
        }
        
    }
    printf("sum=%d",sum);
    return 0;
    
}
